><td align="left"><bold>FV</bold></td><td align="center">t</td><td align="center">g</td><td align="center">g/a</td><td align="center">t</td><td align="center">g/a</td><td align="center">t</td><td align="center">a</td><td align="center">g/a</td><td align="center">t</td><td align="center">g</td></tr><tr><td align="left"><bold>HF</bold></td><td align="center">t</td><td align="center">g</td><td align="center">g</td><td align="center">t</td><td align="center">a</td><td align="center">t</td><td align="center">a</td><td align="center">g</td><td align="center">t</td><td align="center">g</td></tr><tr><td align="left"><bold>LI</bold></td><td align="center">t</td><td align="center">g</td><td align="center">g</td><td align="center">t</td><td align="center">g/a</td><td align="center">t</td><td align="center">a</td><td align="center">g</td><td align="center">t</td><td align="center">g</td></tr><tr><td colspan="11"><hr></hr></td></tr><tr><td align="left"><bold>NE</bold></td><td align="center">g/t</td><td align="center">g/a</td><td align="center">g</td><td align="center">t</td><td align="center">g</td><td align="center">a</td><td align="center">a</td><td align="center">g</td><td align="center">a/t</td><td align="center">g</td></tr><tr><td align="left"><bold>SW</bold></td><td align="center">g</td><td align="center">a</td><td align="center">g</td><td align="center">t</td><td align="center">g</td><td align="center">a</td><td align="center">c</td><td align="center">g</td><td align="center">a</td><td align="center">g</td></tr><tr><td align="left"><bold>BH</bold></td><td align="center">g</td><td align="center">g/a</td><td align="center">g</td><td align="center">t</td><td align="center">g</td><td align="center">a/t</td><td align="center">a</td><td align="center">g</td><td align="center">a</td><td align="center">g</td></tr><tr><td colspan="11"><hr></hr></td></tr><tr><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td><td></td></tr><tr><td colspan="11">
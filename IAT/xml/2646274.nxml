d="A5" contrib-type="author"><name><surname>Li</surname><given-names>Li</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>l3li@stanford.edu</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Kodama</surname><given-names>Keiichi</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><email>kkodama@stanford.edu</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Chiang</surname><given-names>Annie P</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><email>chianga@stanford.edu</email></contrib><contrib id="A8" corresp="yes" contrib-type="author"><name><surname>Butte</surname><given-names>Atul J</given-names></name><xref ref-type="aff" rid="I1">1</xref><xref ref-type="aff" rid="I2">2</xref><xref ref-type="aff" rid="I3">3</xref><email>abutte@stanford.edu</email></contrib></contrib-group><aff id="I1"><label>1</label>Stanford Center for Biomedical Informatics Research, 251 Cmpus Drive, Stanford, CA 94305, USA</aff><aff id="I2"><label>2</label>Department of Pediatrics, Stanford University School of Medicine, Stanford, CA 94305, USA</aff><aff id="I3"><label>3</label>Lucile Packard Children's Hospital, 725 Welch Road, Palo Alto, CA 94304, USA</aff><aff id="I4"><label>4</label>NuMedii Inc., Menlo Park, CA 94025, USA</aff><pub-date pub-type="ppub"><year>2008</year></pub-date><pub-date pub-type="epub"><day>5</day><month>12</month><year>2008</year></pub-date><volume>9</volume><issue>12</issue><fpage>R170</fpage><lpage>R170</lpage><ext-link ext-link-type="uri" xlink:href="http://genomebiology.com/2008/9/12/R170"/><history><date date-type="received"><day>17</day><month>6</month><year>2008</year></date><date date-type="rev-recd"><day>26</day><month>9</month><year>2008</year></date><date date-type="accepted"><day>5</day><month>12</month><year>2008</year></date></history><
contrib-type="author"><name><surname>Pierlot</surname><given-names>C&#x000e9;line</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>celine@polyarthrite.net</email></contrib><contrib id="A6" contrib-type="author"><name><surname>Migliorini</surname><given-names>Paola</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>p.migliorini@med.unipi.it</email></contrib><contrib id="A7" contrib-type="author"><name><surname>Balsa</surname><given-names>Alejandro</given-names></name><xref ref-type="aff" rid="I6">6</xref><email>abalsa@hulp.insalud.es</email></contrib><contrib id="A8" contrib-type="author"><name><surname>Westhovens</surname><given-names>Ren&#x000e9;</given-names></name><xref ref-type="aff" rid="I7">7</xref><email>rene.westhovens@uz.kuleuven.ac.be</email></contrib><contrib id="A9" contrib-type="author"><name><surname>Barrera</surname><given-names>Pilar</given-names></name><xref ref-type="aff" rid="I8">8</xref><email>p.barrera@reuma.azn.nl</email></contrib><contrib id="A10" contrib-type="author"><name><surname>Alves</surname><given-names>Helena</given-names></name><xref ref-type="aff" rid="I9">9</xref><email>h.alves@chnorte.min-saude.pt</email></contrib><contrib id="A11" contrib-type="author"><name><surname>Vaz</surname><given-names>Carlos</given-names></name><xref ref-type="aff" rid="I9">9</xref><email>carlosvaz@med.up.pt</email></contrib><contrib id="A12" contrib-type="author"><name><surname>Fernandes</surname><given-names>Manuela</given-names></name><xref ref-type="aff" rid="I9">9</xref><email>h.alves@chnorte.min-saude.pt</email></contrib><contrib id="A13" contrib-type="author"><name><surname>Pascual-Salcedo</surname><given-names>Dora</given-names></name><xref ref-type="aff" rid="I6">6</xref><email>abalsa@hulp.insalud.es</email></contrib><contrib id="A14" contrib-type="author"><name><surname>Bombardieri</surname><given-names>Stefano</given-names></name><xref ref-type="aff" rid="I5">5</xref><email>s.bombardieri@int.med.unipi.it</email></c
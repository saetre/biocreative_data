uthor"><name><surname>Chen</surname><given-names>Wei-Min</given-names></name><xref rid="aff3" ref-type="aff">3</xref></contrib><contrib contrib-type="author"><name><surname>Uda</surname><given-names>Manuela</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><contrib contrib-type="author"><name><surname>Albai</surname><given-names>Giuseppe</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><contrib contrib-type="author"><name><surname>Strait</surname><given-names>James</given-names></name><xref rid="aff2" ref-type="aff">2</xref></contrib><contrib contrib-type="author"><name><surname>Najjar</surname><given-names>Samer</given-names></name><xref rid="aff2" ref-type="aff">2</xref></contrib><contrib contrib-type="author"><name><surname>Nagaraja</surname><given-names>Ramaiah</given-names></name><xref rid="aff2" ref-type="aff">2</xref></contrib><contrib contrib-type="author"><name><surname>Orr&#x000fa;</surname><given-names>Marco</given-names></name><xref rid="aff4" ref-type="aff">4</xref><xref rid="aff5" ref-type="aff">5</xref></contrib><contrib contrib-type="author"><name><surname>Usala</surname><given-names>Gianluca</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><contrib contrib-type="author"><name><surname>Dei</surname><given-names>Mariano</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><contrib contrib-type="author"><name><surname>Lai</surname><given-names>Sandra</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><contrib contrib-type="author"><name><surname>Maschio</surname><given-names>Andrea</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><contrib contrib-type="author"><name><surname>Busonero</surname><given-names>Fabio</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><contrib contrib-type="author"><name><surname>Mulas</surname><given-names>Antonella</given-names></name><xref rid="aff4" ref-type="aff">4</xref></contrib><con